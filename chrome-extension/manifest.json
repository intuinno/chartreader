{
				"manifest_version": 2, 
				"name": "Getting started Example",
				"description": "Deok's first tutorial",
				"version": "1.0",

				"browser_action": {
								"default_icon": "icon.png",
								"default_popup": "popup.html",
								"default_title": "Hello Deokgun"

				}, 



				"content_scripts": [
					{
						"matches":["<all_urls>"],
						"js": ["jquery.js", "myscript.js"]
					}
				],

				"permissions": [
								"activeTab", 
								"storage"
				]
}
